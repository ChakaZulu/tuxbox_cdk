+u8 *dhcp_vendorex_prep (u8 *e)
+{
+	const char *part1 = "DBOX2, ";
+	const char *part2 = id2name[mid];
+
+	/* DHCP vendor-class-identifier = 60 */
+	*e++ = 60;
+	*e++ = strlen (part1) + strlen (part2);
+	while (*part1)
+		*e++ = *part1++;
+	while (*part2)
+		*e++ = *part2++;
+
+	return e;
+}
