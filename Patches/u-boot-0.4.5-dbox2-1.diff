+	x5 = (x8-(W1+W7)*x5)>>12;
+	x8 = W3*(x6+x7) + 2048;
+	x6 = (x8-(W3-W5)*x6)>>12;
+	x7 = (x8-(W3+W5)*x7)>>12;
+
+	x8 = x0 + x1;
+	x0 -= x1;
+	x1 = W6*(x3+x2) + 2048;
+	x2 = (x1-(W2+W6)*x2)>>12;
+	x3 = (x1+(W2-W6)*x3)>>12;
+	x1 = x4 + x6;
+	x4 -= x6;
+	x6 = x5 + x7;
+	x5 -= x7;
