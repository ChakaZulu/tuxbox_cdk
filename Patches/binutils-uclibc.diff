+    targ_selvecs="arm_epoc_pe_little_vec arm_epoc_pe_big_vec arm_epoc_pei_little_vec arm_epoc_pei_big_vec"
+    targ_underscore=no
+    targ_cflags=-DARM_COFF_BUGFIX
+    ;;
+  arm-wince-pe | arm-*-wince)
