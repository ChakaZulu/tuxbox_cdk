+    case "${host}" in
+      i[[3456789]]86-*-mingw32) ;; # keep gdb tcl tk expect etc.
+      *) noconfigdirs="$noconfigdirs gdb tcl tk expect itcl tix gnuserv"
+         ;;
+    esac
+    ;;    
+  *-*-cygwin*)
